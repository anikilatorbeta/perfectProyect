                <p:menuitem value="Perfiles Tarjetas" icon="ui-icon-note" url="perfiles.xhtml"/>

                <p:menuitem value="Clientes" icon="ui-icon-person" url="#"/>
                <p:menuitem value="Transacciones" icon="ui-icon-comment" url="#"/>

                <p:menuitem value="Estadisticas" icon="ui-icon-image" url="#"/>


                <p:menuitem value="Usuarios" icon="ui-icon-key" url="#"/>

                <f:facet name="options">
                  
                    <div style="margin-right:20px">
                    <p:clock pattern="HH:mm:ss dd-MM-yyyy" mode="server" /> 
                    </div>
                   
                </f:facet>

            </p:menubar>
        </ui:composition>
    </h:body>

</html>
